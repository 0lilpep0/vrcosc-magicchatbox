<Window x:Class="vrcosc_magicchatbox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:vrcosc_magicchatbox"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="{Binding PlayingSongTitle}" HorizontalAlignment="Center" Width="auto"/>
            <Button Click="GetSongTitle" Height="22" Width="80" Content="Get Song title"/>
        </StackPanel>


    </Grid>
</Window>
